{
  "name": "mountebank-openapi-formatter",
  "version": "0.0.1",
  "author": "Opus Software",
  "description": "Custom formatter to read an openapi file on mountebank",
  "license": "MIT",
  "main": "./src/formatter.js",
  "repository": {
